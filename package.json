{
  "name": "sky-sentry",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage --config ./jest.config.ts",
    "test:watch": "jest --watch",
    "prettier": "npx prettier --write .",
    "eslint": "npx eslint --ext .ts",
    "commitlint": "npx commitlint --edit"
